iles\/civicrm\/ext\/org.civicrm.volunteer\/ang\/volunteer\/VolOppsCtrl.js"],"css":["https:\/\/civi.ddev.site\/\/sites\/default\/files\/civicrm\/ext\/org.civicrm.volunteer\/ang\/volunteer.css"],"partials":{"~\/volunteer\/Project.html":"<div class=\"crm-vol-project\" crm-vol-perm-to-class>\n  <div crm-ui-debug=\"project\"><\/div>\n  <div crm-ui-debug=\"relationships\"><\/div>\n  <div crm-ui-debug=\"profiles\"><\/div>\n  <div crm-ui-debug=\"locBlock\"><\/div>\n  <div crm-ui-debug=\"campaigns\"><\/div>\n  <div crm-ui-debug=\"countries\"><\/div>\n  <div crm-ui-debug=\"supporting_data\"><\/div>\n  <div crm-ui-debug=\"relationships\"><\/div>\n  <div id=\"backboneIncludes\"><\/div>\n\n  <div class=\"help\" ng-show=\"supporting_data.volunteer_general_project_settings_help_text\"\n       ng-bind-html=\"supporting_data.volunteer_general_project_settings_help_text\"><\/div>\n  <form class=\"crm-form-block\" name=\"projectForm\" crm-ui-id-scope>\n\n    <fieldset class=\"crm-vol-project-settings\" crm-ui-id-scope>\n      <legend>{{ts(\"Project Settings\")}}<\/legend>\n      <div class=\"crm-vol-title\" crm-ui-field=\"{name: 'projectForm.title', title: ts('Project Title')}\">\n        <input class=\"big crm-form-text\" crm-ui-id=\"projectForm.title\" ng-model=\"project.title\" ng-required=\"true\" \/>\n      <\/div>\n\n      <div class=\"crm-vol-description\" crm-ui-field=\"{name: 'projectForm.description', title: ts('Project Description')}\">\n        <p>\n          <span class=\"crm-button crm-icon-button crm-vol-button crm-vol-preview-description crm-vol-project-preview-description\" ng-click=\"previewDescription()\">\n            <span class=\"crm-button-icon ui-icon-comment\"> <\/span>\n            <span class=\"crm-button-text\">{{ts('Preview Description')}}<\/span>\n          <\/span>\n        <\/p>\n        <div id=crm-vol-form-textarea-wrapper>\n          <textarea crm-ui-richtext class=\"big crm-form-textarea\" crm-ui-id=\"projectForm.description\" ng-model=\"project.description\"><\/textarea>\n        <\/div>\n      <\/div>\n\n      <div class=\"crm-vol-campaign\" crm-ui-field='{name: \"projectForm.campaign_id\", title: ts(\"Campaign\")}'>\n        <select class=\"big crm-form-select crm-vol-campaign\" crm-ui-select=\"{placeholder: '', allowClear:true}\"\n                ng-options=\"key as value.title for (key , value) in campaigns track by key\" ng-model=\"project.campaign_id\">\n          <option \/>\n        <\/select>\n      <\/div>\n\n      <div class=\"crm-vol-location\" crm-ui-field='{name: \"projectForm.locBlock\", title: ts(\"Location\")}'>\n        <select class=\"big crm-form-select crm-vol-location-block-id\"\n                crm-ui-select=\"{placeholder: '', allowClear:true}\"\n                ng-options=\"key as value for (key , value) in locationBlocks track by key\" ng-model=\"project.loc_block_id\">\n          <option \/>\n        <\/select>\n        <div id=\"crm-vol-location-block\" class=\"crm-vol-extra-top\" crm-ui-accordion=\"{title: ts('Edit Location'), collapsed: project.loc_block_id !==0}\" ng-hide=\"!project.loc_block_id\">\n          <div class=\"status\" ng-show=\"locBlock.count_loc_used > 0\">{{ts(\"This location is used by other events\/projects. Modifying location information will change values for all events\/projects\")}}<\/div>\n          <fieldset class=\"crm-vol-location-address\">\n            <legend>{{ts(\"Address\")}}<\/legend>\n            <div class=\"crm-vol-location-name\" crm-ui-field='{name: \"projectForm.locBlock.name\", title: ts(\"Address Name\")}'>\n\u00a0 \u00a0 \u00a0 \u00a0 \u00a0 \u00a0 \u00a0 <input size=\"30\" crm-ui-id=\"projectForm.locBlock.name\" \u00a0ng-model=\"locBlock.address.name\" \/>\n \u00a0 \u00a0 \u00a0 \u00a0 \u00a0 \u00a0<\/div>\n            <div class=\"crm-vol-location-street\" crm-ui-field='{name: \"projectForm.locBlock.street_address\", title: ts(\"Street Address\")}'>\n              <input size=\"30\" crm-ui-id=\"projectForm.locBlock.street_address\"  ng-model=\"locBlock.address.street_address\" \/>\n            <\/div>\n            <div class=\"crm-vol-location-sup1\" crm-ui-field='{name: \"projectForm.locBlock.supplemental_address_1\", title: ts(\"Supplemental Address 1\")}'>\n              <input size=\"30\" crm-ui-id=\"projectForm.locBlock.supplemental_address_1\" ng-model=\"locBlock.address.supplemental_address_1\" \/>\n            <\/div>\n            <div class=\"crm-vol-location-sup2\" crm-ui-field='{name: \"projectForm.locBlock.supplemental_address_2\", title: ts(\"Supplemental Address 2\")}'>\n              <input size=\"30\" crm-ui-id=\"projectForm.locBlock.supplemental_address_2\" ng-model=\"locBlock.address.supplemental_address_2\" \/>\n            <\/div>\n            <div class=\"crm-vol-location-city\" crm-ui-field='{name: \"projectForm.locBlock.city\", title: ts(\"City\")}'>\n              <input crm-ui-id=\"projectForm.locBlock.city\" ng-model=\"locBlock.address.city\" \/>\n            <\/div>\n            <div class=\"crm-vol-location-postal\" crm-ui-field='{name: \"projectForm.locBlock.postal\", title: ts(\"Postal Code - Suffix\")}'>\n              <input size=\"9\" crm-ui-id=\"projectForm.locBlock.postal\" ng-model=\"locBlock.address.postal_code\" \/>\n              -\n              <input size=\"5\" crm-ui-id=\"projectForm.locBlock.postal_code_suffix\" ng-model=\"locBlock.address.postal_code_suffix\" \/>\n            <\/div>\n            <div class=\"crm-vol-location-country_id\" crm-ui-field=\"{name: 'projectForm.locBlock.country_id', title: ts('Country')}\">\n              <select\n                class=\"big crm-form-select\"\n                crm-ui-id=\"projectForm.locBlock.country_id\"\n                crm-ui-select=\"{allowClear: false}\"\n                ng-change=\"locBlockDirty()\"\n                ng-model=\"locBlock.address.country_id\"\n                ng-options=\"k as v.name for (k, v) in countries track by k\">\n              <\/select>\n            <\/div>\n          <\/fieldset>\n          <fieldset class=\"crm-vol-location-phone\">\n            <legend>{{ts(\"Phone\")}}<\/legend>\n            <div class=\"crm-vol-location-phone1\" crm-ui-field='{name: \"projectForm.locBlock.phone1\", title: ts(\"Phone 1\")}'>\n              <input crm-ui-id=\"projectForm.locBlock.phone1\"  ng-model=\"locBlock.phone.phone\" \/>\n              {{ts(\"ext.\")}} <input size=\"5\"  ng-model=\"locBlock.phone.phone_ext\" \/>\n              <select crm-ui-select=\"{width: '100', allowClear: true}\" ng-model=\"locBlock.phone.phone_type_id\">\n                <option \/>\n                <option ng-repeat=\"(pTypeId, pType) in phone_types\" value=\"{{pTypeId}}\">{{pType}}<\/option>\n              <\/select>\n            <\/div>\n            <div class=\"crm-vol-location-phone2\" crm-ui-field='{name: \"projectForm.locBlock.phone2\", title: ts(\"Phone 2\")}'>\n              <input crm-ui-id=\"projectForm.locBlock.phone2\" ng-model=\"locBlock.phone_2.phone\" \/>\n              {{ts(\"ext.\")}} <input size=\"5\" ng-model=\"locBlock.phone_2.phone_ext\" \/>\n              <select crm-ui-select=\"{width: '100', allowClear: true}\" ng-model=\"locBlock.phone_2.phone_type_id\">\n                <option \/>\n                <option ng-repeat=\"(pTypeId, pType) in phone_types\" value=\"{{pTypeId}}\">{{pType}}<\/option>\n              <\/select>\n            <\/div>\n          <\/fieldset>\n          <fieldset class=\"crm-vol-location-email\">\n            <legend>{{ts(\"Email\")}}<\/legend>\n            <div class=\"crm-vol-location-email1\" crm-ui-field='{name: \"projectForm.locBlock.email1\", title: ts(\"Email 1\")}'>\n              <input size=\"30\" type=\"email\" crm-ui-id=\"projectForm.locBlock.email1\" ng-model=\"locBlock.email.email\" \/>\n            <\/div>\n            <div class=\"crm-vol-location-email2\" crm-ui-field='{name: \"projectForm.locBlock.email2\", title: ts(\"Email 2\")}'>\n              <input size=\"30\" type=\"email\" crm-ui-id=\"projectForm.locBlock.email2\" ng-model=\"locBlock.email_2.email\" \/>\n            <\/div>\n          <\/fieldset>\n        <\/div>\n      <\/div>\n\n\n      <div class=\"crm-vol-is_active\" crm-ui-field=\"{name: 'projectForm.is_active', title: ts('Is this Project Active?')}\">\n        <input type=\"checkbox\" crm-ui-id=\"projectForm.is_active\" ng-model=\"project.is_active\" \/>\n      <\/div>\n    <\/fieldset>\n\n    <fieldset class=\"crm-vol-relationships\" ng-if=\"showRelationshipBlock\" crm-ui-id-scope>\n      <legend>{{ts(\"Relationships\")}}<\/legend>\n      <div ng-repeat=\"relType in relationship_types\">\n        <div crm-ui-field=\"{name: projectForm[relType.name], title: ts(relType.label)}\" ng-class=\"'crm-vol-rel-' + relType.name\">\n          <input crm-ui-id=\"projectForm[relType.name]\" crm-entityref=\"{entity: 'Contact', select: {allowClear:true, multiple: true}}\" ng-model=\"relationships[relType.value]\" \/>\n          <div class=\"description\" ng-show=\"relType.description\">{{relType.description}}<\/div>\n        <\/div>\n      <\/div>\n    <\/fieldset>\n\n    <fieldset class=\"crm-vol-profiles\" ng-if=\"showProfileBlock\">\n      <legend>{{ts(\"Volunteer Registration\")}}<\/legend>\n      <div class=\"help\">\n        <p>\n          {{ts(\"What questions would you like to ask your volunteers? Select one or more Profiles below to tailor the registration forms for this project.\")}}\n        <\/p>\n        <p>\n          {{ts('You may create up to two sign up forms for a volunteer project: one for individual registrations and another for group registrations. The \"Use For\" field specifies whether the Profile should be used for individual registrations, group registrations, or both. For group registrations, it is recommended that a single, short Profile be used.')}}\n        <\/p>\n      <\/div>\n      <div id=\"org_civicrm_volunteer-sign-up-profiles\">\n        <div ng-repeat=\"profile in profiles\" class='crm-profile-selector-container'>\n          <a ng-click=\"removeProfile($index)\" class=\"crm-hover-button crm-button-remove-profile\">\n            <span class=\"icon ui-icon-circle-close\"><\/span>\n          <\/a>\n\n          <div class=\"crm-profile-selector-wrapper crm-section\">\n            <div class=\"label\">\n              <label>{{ts(\"Profile:\")}}<\/label>\n            <\/div>\n            <div class=\"content\">\n              <input ng-if=\"supporting_data.use_profile_editor\" crm-profile-selector=\"{}\"\n                     ng-model=\"profile.uf_group_id\" ng-change=\"validateProfileSelections()\"\/>\n\n              <select ng-if=\"!supporting_data.use_profile_editor\" class=\"big crm-form-select crm-vol-simple-profile-selector\"\n                      crm-ui-select=\"{placeholder: '', allowClear:true}\" ng-change=\"validateProfileSelections()\"\n                      ng-options=\"item as item.title for item in supporting_data.profile_list track by item.id\" ng-model=\"profile.uf_group_id\">\n                <option \/>\n              <\/select>\n            <\/div>\n          <\/div>\n\n          <div class=\"crm-volunteer-profile-use-type crm-section\">\n            <div class=\"label\">\n              <label>{{ts(\"Use For:\")}}<\/label>\n            <\/div>\n            <div class=\"content\">\n              <select ng-model=\"profile.module_data.audience\" crm-ui-select=\"{width: 'style'}\" ng-change=\"validateProfileSelections()\">\n                <option ng-repeat=\"audience in supporting_data.profile_audience_types\" value=\"{{audience.type}}\">{{audience.label}}<\/option>\n              <\/select>\n            <\/div>\n          <\/div>\n\n          <hr \/>\n        <\/div>\n      <\/div>\n      <a ng-click=\"addProfile()\" class=\"crm-hover-button crm-button-add-profile\" style=\"display: inline-block;\">\n        <span class=\"icon ui-icon-plus\"><\/span>add another profile\n      <\/a>\n    <\/fieldset>\n\n    <div class=\"crm-submit-buttons crm-vol-project-submit-buttons\">\n      <span class=\"crm-button crm-icon-button crm-vol-button crm-vol-save-next crm-vol-project-save-next\" ng-click=\"saveAndNext()\">\n        <span class=\"crm-button-icon ui-icon-triangle-1-e\"> <\/span>\n        <span class=\"crm-button-text\">{{saveAndNextLabel}}<\/span>\n      <\/span>\n      <span ng-show=\"formContext === 'standAlone'\" class=\"crm-button crm-icon-button crm-vol-button crm-vol-save-done crm-vol-project-save-done\" ng-click=\"saveAndDone()\">\n        <span class=\"crm-button-icon ui-icon-circle-check\"> <\/span>\n        <span class=\"crm-button-text\">{{ts('Save and Done')}}<\/span>\n      <\/span>\n      <span class=\"crm-button crm-icon-button crm-button-type-cancel crm-vol-button crm-vol-cancel crm-vol-project-cancel\" ng-click=\"cancel()\">\n        <span class=\"crm-button-icon ui-icon-close\"> <\/span>\n        <span class=\"crm-button-text\">{{ts('Cancel')}}<\/span>\n      <\/span>\n    <\/div>\n\n  <\/form>\n\n<\/div>\n","~\/volunteer\/Projects.html":"<div class=\"crm-container\">\n  <!-- TODO for VOL-267: Remove beneficiaries debugging block. -->\n  <div crm-ui-debug=\"beneficiaries\"><\/div>\n  <div crm-ui-debug=\"campaigns\"><\/div>\n  <div crm-ui-debug=\"projects\"><\/div>\n  <div crm-ui-debug=\"searchParams\"><\/div>\n\n  <div class=\"help\" ng-show=\"!canAccessAllProjects\">\n    <p>\n      {{ts(\"This page displays only the volunteer projects which you have access to edit.\")}}\n      <!-- not translated because HTML tags in a ts() choke Angular -->\n      For a list of all volunteer opportunities on the site, visit the <a href=\"{{urlPublicVolOppSearch}}\">public search form<\/a>.\n    <\/p>\n  <\/div>\n  <div class=\"crm-form-block\">\n    <div class=\"crm-vol-widgets crm-vol-manage-widgets\"><\/div>\n    <div class=\"clear\"><\/div>\n    <div class=\"crm-vol-buttons crm-vol-mange-buttons crm-vol-half-responsive\">\n\n      <a href=\"#\/volunteer\/manage\/0\" class=\"button\"><span><div class=\"icon ui-icon-plus\"><\/div>{{ts('Add Project')}}<\/span><\/a><br \/><br \/>\n\n      <select crm-ui-select=\"{placeholder: ts('Bulk Actions'), allowClear: true, width: '150'}\" ng-model=\"batchAction\" id=\"batchAction\">\n        <option \/>\n        <option ng-repeat=\"(key, action) in batchActions\" value=\"{{key}}\">{{action.label}}<\/option>\n      <\/select>\n      <input type=\"button\" value=\"{{ts('Run')}}\" ng-click=\"runBatch()\" \/>\n    <\/div>\n    <div class=\"crm-vol-search crm-vol-manage-search crm-vol-half-responsive\">\n      <form name=\"volProjectSearchForm\" crm-ui-id-scope>\n        <fieldset class=\"crm-vol-ui-fieldset ui-corner-all\">\n          <legend>Find Volunteer Projects<\/legend>\n          <div class=\"crm-group\" crm-ui-id-scope>\n            <div class=\"crm-vol-field crm-vol-field-projects-search-title\" crm-ui-field=\"{name: 'volProjectSearchForm.title', title: ts('Title')}\">\n              <input crm-ui-id=\"volProjectSearchForm.title\" ng-model=\"searchParams.title\" \/>\n            <\/div>\n            <div class=\"crm-vol-field crm-vol-field-projects-search-beneficiaries\" crm-ui-field=\"{name: 'volProjectSearchForm.beneficiary', title: ts('Beneficiary')}\">\n              <!-- TODO for VOL-267: Replace this with an entityRef widget. -->\n              <select class=\"big crm-form-select crm-vol-beneficiary\" crm-ui-select=\"{placeholder: ts('Filter by Beneficiary'), allowClear:true}\"\n                      ng-options=\"key as value.display_name for (key , value) in beneficiaries track by key\" ng-model=\"searchParams.beneficiaries\">\n                <option \/>\n              <\/select>\n            <\/div>\n            <div class=\"crm-vol-field crm-vol-field-projects-search-campaign\" crm-ui-field=\"{name: 'volProjectSearchForm.campaign_id', title: ts('Campaign')}\">\n              <select class=\"big crm-form-select crm-vol-campaign\" crm-ui-select=\"{placeholder: ts('Filter by Campaign'), allowClear:true}\"\n                      ng-change=\"clearCampaign()\"\n                      ng-options=\"key as value.title for (key , value) in campaigns  track by key\" ng-model=\"searchParams.campaign_id\">\n                <option \/>\n              <\/select>\n            <\/div>\n            <div class=\"crm-vol-field crm-vol-field-projects-search-active\" crm-ui-field=\"{name: 'volProjectSearchForm.is_active', title: ts('Active')}\">\n              <label>\n                <input type=\"radio\" ng-model=\"searchParams.is_active\" value=\"1\" \/> Yes\n              <\/label>\n              <label>\n                <input type=\"radio\" ng-model=\"searchParams.is_active\" value=\"0\" \/> No\n              <\/label>\n            <\/div>\n          <\/div>\n        <\/fieldset>\n      <\/form>\n    <\/div>\n\n\n    <div class=\"clear\"><\/div>\n    <table id=\"crm-vol-project-list\" class=\"display dataTable no-footer\" role=\"grid\">\n      <thead>\n        <tr role=\"row\">\n          <th class=\"ui-state-default crm-vol-manage-toggle\"><input type=\"checkbox\" ng-model=\"allSelected\" ng-change=\"selectAll()\" \/><\/th>\n          <th class=\"ui-state-default crm-vol-manage-project\">{{ ts('Volunteer Project') }}<\/th>\n          <th class=\"ui-state-default crm-vol-manage-entity\">{{ ts('Associated Entity') }}<\/th>\n          <th class=\"ui-state-default crm-vol-manage-beneficiaries\">{{ ts('Beneficiaries') }}<\/th>\n          <th class=\"ui-state-default crm-vol-manage-location\">{{ ts('Location') }}<\/th>\n          <th class=\"ui-state-default crm-vol-manage-active\">{{ ts('Active') }}<\/th>\n          <th class=\"ui-state-default crm-vol-manage-actionlinks\"><\/th>\n        <\/tr>\n      <\/thead>\n      <tbody>\n      <tr ng-repeat=\"project in projects | filter:searchParams\" role=\"row\">\n        <td class=\"crm-vol-manage-toggle\"><input type=\"checkbox\" ng-model=\"project.selected\" ng-change=\"watchSelected()\" \/><\/td>\n        <td class=\"crm-vol-manage-project\">\n          <span class=\"crm-vol-manage-project-title\">{{ project.title }}<\/span>\n          <span class=\"crm-vol-manage-project-id\">{{ ts('(ID: %1)', {1: project.id}) }}<\/span>\n        <\/td>\n        <td class=\"crm-vol-manage-entity\">\n          <a ng-if=\"canLinkToAssociatedEntity(project)\" ng-click=\"linkToAssociatedEntity(project)\">{{associatedEntityTitle(project)}}<\/a>\n          <span ng-if=\"!canLinkToAssociatedEntity(project)\">{{associatedEntityTitle(project)}}<\/span>\n        <\/td>\n        <td class=\"crm-vol-manage-beneficiaries\" ng-bind-html=\"formatBeneficiaries(project)\"><\/td>\n        <td class=\"crm-vol-manage-location\" ng-bind-html=\"formatLocation(project)\"><\/td>\n        <td class=\"crm-vol-manage-active\">{{ project.is_active == 1 ? ts(\"Yes\") : ts(\"No\") }}<\/td>\n        <td class=\"crm-vol-manage-actionlinks right nowrap\">\n          <div><a href=\"#\/volunteer\/manage\/{{project.id}}\">{{ ts('Edit') }}<\/a><\/div>\n          <div><a ng-click=\"backbonePopup(ts('Define Volunteer Opportunities'), 'Define', project.id)\">{{ ts('Define Volunteer Opportunities') }}<\/a><\/div>\n          <div><a ng-click=\"backbonePopup(ts('Assign Volunteers'), 'Assign', project.id)\">{{ ts('Assign Volunteers') }}<\/a><\/div>\n          <div><a ng-click=\"showRoster()\">{{ ts('View Volunteer Roster') }}<\/a><\/div>\n          <div><a ng-click=\"showLogHours()\">{{ ts('Log Hours') }}<\/a><\/div>\n          <div><a target=\"_blank\" href=\"#\/volunteer\/opportunities?project={{project.id}}&amp;hideSearch=1\">{{ ts('Public Signup') }}<\/a><\/div>\n        <\/td>\n      <\/tr>\n      <\/tbody>\n    <\/table>\n  <\/div>\n<\/div>\n","~\/volunteer\/VolOppsCtrl.html":"<!--\nController: VolOppsCtrl\nRequired vars: volOppData(), searchParams\n-->\n\n<div class=\"crm-container\">\n  <div crm-ui-debug=\"volOppData()\"><\/div>\n  <div crm-ui-debug=\"searchParams\"><\/div>\n  <div crm-ui-debug=\"proximityUnits\"><\/div>\n  <div crm-ui-debug=\"countries\"><\/div>\n\n  <div class=\"help\">\n    <p>\n      {{helpText()}}\n    <\/p>\n  <\/div>\n\n  <!-- Search Form -->\n  <div class=\"crm-vol-half-responsive crm-vol-opp-show-hidden-search\" ng-show=\"allowShowSearch\">\n    <div class=\"crm-buttons\">\n      <button crm-icon=\"search\" ng-click=\"showSearch()\">{{ts('Filter this List of Volunteer Opportunities')}}<\/button>\n    <\/div>\n  <\/div>\n  <div class=\"crm-vol-half-responsive crm-vol-opp-search\" ng-hide=\"hideSearch\">\n    <h2>{{ts('Find Volunteer Opportunities')}}<\/h2>\n    <form name=\"volOppSearchForm\" crm-ui-id-scope>\n      <div class=\"crm-block ui-tabs ui-widget ui-widget-content ui-corner-all\">\n        <div class=\"crm-group\" crm-ui-id-scope>\n          <div crm-ui-field=\"{name: 'volOppSearchForm.date_start', title: ts('Between')}\" class=\"crm-vol-opp-time\">\n            <input crm-ui-id=\"volOppSearchForm.date_start\" crm-ui-datepicker=\"{time: false}\" ng-model=\"searchParams.date_start\" \/>\n            <label crm-ui-for=\"volOppSearchForm.date_end\">and<\/label>\n            <input crm-ui-id=\"volOppSearchForm.date_end\" crm-ui-datepicker=\"{time: false}\" ng-model=\"searchParams.date_end\" \/>\n          <\/div>\n\n          <div crm-ui-field=\"{name: 'volOppSearchForm.role_id', title: ts('Role')}\" class=\"crm-vol-opp-role\">\n            <select\n              crm-ui-id=\"volOppSearchForm.role_id\"\n              crm-ui-select=\"{minimumInputLength: 0, placeholder: ts('Select Volunteer Role(s)')}\"\n              type=\"text\"\n              multiple=\"multiple\"\n              name=\"role_id\"\n              ng-model=\"searchParams.role_id\"\n              ng-options=\"key as value for (key , value) in  roles track by key\"\n              class=\"big crm-form-text\">\n              <option \/>\n            <\/select>\n          <\/div>\n          <div crm-ui-field=\"{name: 'volOppSearchForm.beneficiary', title: ts('With')}\" class=\"crm-vol-opp-beneficiary\">\n            <input\n              crm-entityref=\"{\n                select: {\n                  minimumInputLength: 0,\n                  placeholder: ts('Select Organizer(s)'),\n                  multiple: true\n                },\n                api: {\n                  params: {contact_type: 'Organization'}\n                }\n              }\"\n              crm-ui-id=\"volOppSearchForm.beneficiary\"\n              type=\"text\"\n              name=\"beneficiary\"\n              ng-model=\"searchParams.beneficiary\"\n              class=\"big crm-form-text\"\n            \/>\n          <\/div>\n        <\/div>\n        <div class=\"crm-vol-proximity\" crm-ui-accordion=\"{title: ts('Search by Location'), collapsed: !isProximitySearch}\">\n          <div crm-ui-field=\"{name: 'volOppSearchForm.radius', title: ts('Within')}\" class=\"crm-vol-opp-radius\">\n            <input\n              class=\"four crm-form-text\"\n              crm-ui-id=\"volOppSearchForm.radius\"\n              ng-model=\"searchParams.proximity.radius\"\n              ng-required=\"isProximitySearch\"\n              type=\"number\"\n            \/>\n            <select\n              crm-ui-id=\"volOppSearchForm.unit\"\n              ng-model=\"searchParams.proximity.unit\"\n              ng-options=\"option.value as option.label for option in proximityUnits\"\n              ng-required=\"isProximitySearch\">\n            <\/select>\n            <label crm-ui-for=\"volOppSearchForm.unit\">of:<\/label>\n          <\/div>\n          <div crm-ui-field=\"{name: 'volOppSearchForm.street_address', title: ts('Street Address')}\" class=\"crm-vol-opp-street\">\n            <input\n              class=\"crm-form-text\"\n              crm-ui-id=\"volOppSearchForm.street_address\"\n              ng-model=\"searchParams.proximity.street_address\"\n              type=\"text\"\n            \/>\n          <\/div>\n          <div crm-ui-field=\"{name: 'volOppSearchForm.city', title: ts('City')}\" class=\"crm-vol-opp-city\">\n            <input\n              class=\"crm-form-text\"\n              crm-ui-id=\"volOppSearchForm.city\"\n              ng-model=\"searchParams.proximity.city\"\n              type=\"text\"\n            \/>\n          <\/div>\n          <div crm-ui-field=\"{name: 'volOppSearchForm.postal_code', title: ts('Postal Code')}\" class=\"crm-vol-opp-postal\">\n            <input\n              class=\"crm-form-text\"\n              crm-ui-id=\"volOppSearchForm.postal_code\"\n              ng-model=\"searchParams.proximity.postal_code\"\n              ng-required=\"isProximitySearch\"\n              type=\"text\"\n            \/>\n          <\/div>\n          <div crm-ui-field=\"{name: 'volOppSearchForm.country', title: ts('Country')}\" class=\"crm-vol-opp-country\">\n            <select\n              class=\"big crm-form-select\"\n              crm-ui-id=\"volOppSearchForm.country\"\n              crm-ui-select=\"{placeholder: ts('Select Country'), allowClear:true}\"\n              ng-model=\"searchParams.proximity.country\"\n              ng-options=\"k as v.name for (k, v) in countries track by k\"\n              ng-required=\"isProximitySearch\">\n            <\/select>\n          <\/div>\n        <\/div>\n        <div class=\"crm-buttons\">\n          <button crm-icon=\"search\" ng-click=\"search()\">{{ts('Search')}}<\/button>\n        <\/div>\n      <\/div>\n    <\/form>\n  <\/div>\n\n\n  <!-- Shopping Cart -->\n  <div class=\"crm-vol-half-responsive crm-vol-opp-cart\" ng-class=\"{'floating_cart': cartIsFloating, 'contents_visible': showCartContents}\">\n    <h2>{{ts('Your Selected Volunteer Opportunities')}}<\/h2>\n    <div class=\"crm-block ui-tabs ui-widget ui-widget-content ui-corner-all \" ng-form=\"volOppCheckoutForm\" crm-ui-id-scope>\n      <div class=\"crm-vol-opp-cart-message-wrapper\">\n        <div class=\"crm-vol-opp-cart-large-indicator\">\n          <span>{{itemCountInCart}} {{ts('Commitments')}}<\/span>\n        <\/div>\n        <div class=\"crm-vol-opp-cart-message\">\n          <span ng-show=\"itemCountInCart === 0\">{{ts('Make a commitment by clicking the checkbox for the volunteer opportunity that interests you.')}}<\/span>\n          <span ng-hide=\"itemCountInCart === 0\">{{ts('Great! Make as many selections you like, then click \\'Sign Up!\\' to complete your registration.')}}<\/span>\n        <\/div>\n      <\/div>\n\n      <table ng-show=\"showCartContents\" class=\"display dataTable no-footer crm-vol-opp-cart-list\" ng-class=\"{'contents_visible': showCartContents}\" role=\"grid\">\n        <thead>\n        <tr role=\"row\">\n          <th class=\"ui-state-default crm-vol-opp-project\">{{ts('Project')}}<\/th>\n          <th class=\"ui-state-default crm-vol-opp-role\">{{ts('Role')}}<\/th>\n          <th class=\"ui-state-default crm-vol-opp-time\">{{ts('Time')}}<\/th>\n          <th class=\"ui-state-default crm-vol-opp-toggle\">{{ts('Remove')}}<\/th>\n        <\/tr>\n        <\/thead>\n        <tbody>\n          <tr ng-repeat=\"need in shoppingCart\" role=\"row\" class=\"crm-vol-opp-cart-need-{{need.id}}\">\n            <td class=\"crm-vol-opp-project\">\n            {{need.project.title}}\n              <span\n                class=\"icon ui-icon-comment\"\n                ng-show=\"need.project.description\"\n                ng-click=\"showProjectDescription(need.project)\">\n              <\/span>\n          <\/td>\n          <td class=\"crm-vol-opp-role\">{{need.role_label}}\n              <span\n                class=\"icon ui-icon-comment\"\n                ng-show=\"need.role_description\"\n                ng-click=\"showRoleDescription(need)\">\n              <\/span>\n          <\/td>\n          <td class=\"crm-vol-opp-time\">{{need.display_time}}<\/td>\n          <td class=\"crm-vol-opp-toggle\">\n              <span\n                class=\"icon ui-icon-trash\"\n                ng-checked=\"need.inCart\"\n                ng-click=\"toggleSelection(need)\"\n            <\/span>\n          <\/td>\n        <\/tr>\n        <\/tbody>\n      <\/table>\n\n      <div class=\"crm-buttons\">\n        <button class=\"crm-vol-button crm-vol-opp-cart-sign-up\" crm-icon=\"check\" ng-click=\"checkout()\" ng-disabled=\"itemCountInCart === 0\">\n          {{ts('Sign Up!')}}\n        <\/button>\n        <a class=\"crm-vol-opp-cart-show-contents\" ng-hide=\"itemCountInCart === 0\" ng-click=\"toggleCartList()\">{{showCartContents ? ts('Hide selections') : ts('View selections')}}<\/a>\n      <\/div>\n    <\/div>\n  <\/div>\n\n  <div class=\"clear\"><\/div>\n\n  <!-- Results -->\n  <div class=\"crm-vol-opp-results messages status no-popup\" ng-show=\"_.size(volOppData()) === 0\">\n    <div class=\"icon inform-icon\"><\/div>\n    {{ts('No results found. Please try different search criteria.')}}\n  <\/div>\n  <div class=\"crm-vol-opp-results\" ng-show=\"_.size(volOppData()) > 0\">\n    <h2>{{ts('Available Volunteer Opportunities')}}<\/h2>\n    <p class=\"description\">\n      {{ts('For additional project or role detail, click the corresponding detail icon in the table below.')}}\n      <span class=\"icon ui-icon-comment\"><\/span>\n    <\/p>\n    <table class=\"display dataTable no-footer\" role=\"grid\">\n      <thead>\n      <tr role=\"row\">\n        <th class=\"ui-state-default crm-vol-opp-project\">{{ts('Project')}}<\/th>\n        <th class=\"ui-state-default crm-vol-opp-beneficiary\">{{ts('With')}}<\/th>\n        <th class=\"ui-state-default crm-vol-opp-role\">{{ts('Role')}}<\/th>\n        <th class=\"ui-state-default crm-vol-opp-time\">{{ts('Time')}}<\/th>\n        <th class=\"ui-state-default crm-vol-opp-toggle\">{{ts('Sign up!')}}<\/th>\n      <\/tr>\n      <\/thead>\n      <tbody>\n        <tr ng-repeat=\"need in volOppData()\" role=\"row\" class=\"crm-vol-opp-need-{{need.id}}\">\n          <td class=\"crm-vol-opp-project\">\n          {{need.project.title}}\n            <span\n              class=\"icon ui-icon-comment\"\n              ng-show=\"need.project.description\"\n              ng-click=\"showProjectDescription(need.project)\">\n            <\/span>\n        <\/td>\n        <td class=\"crm-vol-opp-beneficiary\">\n          <span class=\"beneficiary\" ng-repeat=\"ben in need.project.beneficiaries\">{{ben.display_name}}<\/span>\n        <\/td>\n        <td class=\"crm-vol-opp-role\">{{need.role_label}}\n            <span\n              class=\"icon ui-icon-comment\"\n              ng-show=\"need.role_description\"\n              ng-click=\"showRoleDescription(need)\">\n            <\/span>\n        <\/td>\n        <td class=\"crm-vol-opp-time\">{{need.display_time}}<\/td>\n        <td class=\"crm-vol-opp-toggle\">\n          <input\n            type=\"checkbox\"\n            ng-checked=\"need.inCart\"\n            ng-click=\"toggleSelection(need)\"\n            class=\"crm-form-checkbox\"\n          \/>\n        <\/td>\n      <\/tr>\n      <\/tbody>\n    <\/table>\n  <\/div>\n<\/div>\n"},"strings":[]}}